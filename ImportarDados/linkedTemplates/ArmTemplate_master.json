{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "ImportarDados"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'connectionString' de 'AzureBlobStorage'"
		},
		"FtpServer1_password": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'password' de 'FtpServer1'"
		},
		"FtpServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "ftp.vitorrubio.com.br"
		},
		"FtpServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vitorrubio"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI para a conta de armazenamento contendo os modelos do ARM vinculados"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token SAS para a conta de armazenamento contendo modelos do ARM vinculados"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ImportarDados_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_connectionString": {
						"value": "[parameters('AzureBlobStorage_connectionString')]"
					},
					"FtpServer1_password": {
						"value": "[parameters('FtpServer1_password')]"
					},
					"FtpServer1_properties_typeProperties_host": {
						"value": "[parameters('FtpServer1_properties_typeProperties_host')]"
					},
					"FtpServer1_properties_typeProperties_userName": {
						"value": "[parameters('FtpServer1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}